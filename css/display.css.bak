#main{
	width:1000px;
	height:85%;
	border:1px solid white;
	border-radius:2px 2px 2px 2px;
	background:#BFD4C5;
	}
	#left,#middle,#right{
		width:285px; 
		height:100px;
		border-top:1px dashed #6830F3;
		margin:10px 0 0 15px;
		float:left;
		}
		#middle{
			width:360px; 
			}
		#picture,#leftmd{
			width:280px;
			height:260px;
			border:2px solid #4DFFFF;
			position:relative;
			margin:15px 0 30px -10px;
			}
			#picture img{
				width:280px;
				height:235px;
				position:absolute;
				}
				#imgp{
					width:280px;
					height:25px;
					font-size:12px;
					font-size:Î¢ÈíÑÅºÚ;
					color:black;
					margin:-5px 0 0 0px;
					background:purple;
					filter:alpha(opacity:80);opacity:0.9;
					text-align:center;
					position:absolute;
					bottom:0px;
					}
					#imgul{margin:210px 0 0 110px;}
					#imgul li{
						width:13px;
						height:13px;
						border:2px solid #4DFFFF;
						font-size:10px;
						text-align:center;
						position:relative;
						list-style:none;
						float:left;
						cursor:pointer;
						margin:0px 0 0 3px;
						}
						.active{
							background:red;
							}
			.title{
				font-size:20px;
				font-family:Î¢ÈíÑÅºÚ;
				color:purple;
				margin:15px 0 0 0px;
				}
			#newul{
				width:350px;
				height:205px;
				margin:0px 0 80px 0;
				}
			#newul li,#list li{
				width:360px;
				height:30px;
				list-style:none;
				margin:5px 0px 0 -50px;
				position:relative;
				}
				#list li{
					margin:5px 0px 0 -10px;
					}
					#list li a,#newul li a{
						height:30px;
						display:block;
						width:230px;
						text-decoration:none;
						font-size:14px;
						margin-left:5px;
						font-family:Î¢ÈíÑÅºÚ;
						color:black;
						margin:-15px 0 0 25px;
						white-space:nowrap;
						overflow:hidden;
						text-overflow:ellipsis;
						}
						#newul li a:hover{
							color:#4DFFFF;
							}
						#list li a:hover{
								color:#4DFFFF;
								}
						.newul li span{
							width:100px;
							height:30px;
							margin:-29px 0 0 0;
							float:right;
							}
						#newul li acroym{
							width:200px;
							white-space:nowrap;
							text-overflow:ellipsis;
							overflow:hidden;
							}
	#right img,#leftmd img{
		margin:10px 0 0 10px;
		float:left;
		}
		#right p,#leftmd p{
			float:left;
			margin:13px 0 20px 15px;
			}
			ul{
				clear:both;
				list-style:none;
				}
		#mid1{
			width:360px;
			height:280px;
			margin:-30px 0 0 -20px;
			border-bottom:1px dashed red;
			border-top:1px dashed red;
			}
	#mid1 img,#mid1 p{
		float:left;
		margin:13px 0 20px 5px;
		}	
		#mid1 ul{
			margin:15px 0 10px 0;
			}
	.list li{
		margin-left:margin:5px 0 0 -80px;
		
		}	
		.list li a{
			width:200px;
			white-space:nowrap;
			text-overflow:ellipsis;
			overflow:hidden;
			}	
			.right{
				margin:5px 0 0 0px;
				width:280px;
				height:270px;
				border-top:none;
				border:2px solid  #4DFFFF;
				}
			#right0 ul{
				margin:10px 0 0 -15px;
				width:245px;
				height:195px;
				margin-left:5px;
				}
				#right0 ul li{
					top:20px;
					position:relative;
					color:red;
					}
			#right1 ul span,#right0 ul span{
					width:20px;
					height:10px;
					background:	#FF3300;
					border-radius:2px 2px 2px 2px;
					font-size:5px;
					margin:0 4px 0 -13px;
					text-align:center;
					color:yellow;
					}
					#right1{
						margin-top:20px;
						}
	#mainpic{
		width:1000px;
		height:200px;
		border:1px solid white;
	  overflow:hidden;
		}
		#webpic{
		width:4000px;
		height:200px;
	 	position:relative;
		top:0px;
		left:0px;
			}
	#webpic img{
			width:1000px;
			height:200px;
			margin:0 0 0 -5px;
			}